<div class="post">
  <ul>
    <% @users.each do |user| %>
    <li class="row">
      <div class="small-2 columns">
      	 	<%= link_to user_path(user) do %> 
        <img class="pic" src="<%= user.profile_photo %>" />
        <% end %>
      </div>
      <div class="small-10 columns">
        <p><%= user.username %></p>
        <p><%= user.bio %></p>
     </div>
   </li>
   <% end %>
</ul>
</div>